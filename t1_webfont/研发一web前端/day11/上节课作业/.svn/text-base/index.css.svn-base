*{
	margin: 0;
	padding: 0;
}
body{
	font-family: "宋体";
}
a{
	text-decoration: none;
}
.box1{
	height: 100vh;
	background: url(img/ia_100000013.jpg) top center/cover;
}
.hd0{
	height: 60px;
	background: white;
	position: fixed;
	top: 0px;
	width: 100%;
	left: 0px;
	z-index: 99;
	display: none;
}
.hd0 img{
	margin-top: 19.5px;
}
.hd0 img:first-child{
	float: left;
	margin-left: 20px;
}
.hd0 img:nth-child(2){
	position: absolute;
	top: 0px;
	left: 50%;
	width: 100px;
	margin-left:-50px ;
}
.hd0 img:last-child{
	float: right;
	margin-right: 20px;
}
.hd{
	height: 52px;
	line-height: 52px;
	padding-top: 50px;
}
.logo{
	float: left;
	height: 52px;
	margin-left: 40px;
}
.nav{
	float: left;
	margin-left: 20%;
	font-size: 0;
}
.nav a{
	font-size: 16px;
	color: white;
	text-decoration: none;
	margin: 0px 28px;
}
.list{
	float: right;
	margin-right: 59px;
	list-style: none;
}
.list li{
	float: left;
	margin: 0px 11px;
	position: relative;
	cursor: pointer;/*改变鼠标形状*/
}
.list li:hover .car{
	display: block;
}
.car{
	position: absolute;
	top: 38px;
	left: -320px;
	width:305px;
	height: 90px;
	background: white;
	padding: 0px 20px 10px;
	display: none;
}
.car p{
	color: #999;
	margin:10px 0px 20px;
}
.oh3{
	font-size: 60px;
	text-align: center;
	color: white;
	clear: both;
	margin-top: 200px;
	font-weight: 500;
	letter-spacing: 10px;
}
.a1{
	width: 160px;
	height: 60px;
	color: white;
	text-align: center;
	line-height: 60px;
	background: #2d2d2d;
	margin: 80px auto 0px;
	display: block;
	opacity: 1;
	transition: all 0.5s;
}
.a1:hover{
	opacity: 0.8;
}
.box2{
	padding: 125px 0px 148px;
}
.box2-con{
	width: 880px;
	margin: 0 auto;
}
.box2-con h3{
	font-size: 36px;
    font-family: "times new roman";
    line-height: 44px;
    font-weight: 500;
}
.box2-con p{
	color: rgb(119, 119, 119);
    font-size: 13px;
    letter-spacing: 1;
    line-height: 28px;
    margin-top: 30px;
    margin-bottom: 20px;
}
.box2-con a{
	color: #d39850;
	padding-right: 10px;
	padding-bottom: 5px;
	font-size: 13px;
	border-bottom:dashed 1px #d39850 ;
}
.box3{
	height:89vw;
	position: relative;
}
.box3>div{
	overflow: hidden;
}
.box31{
	width: 66%;
	height: 100%;
	position: absolute;
	top: 0px;
	left: 0px;
	background: gold;
}
.box32{
	width: 34%;
	height: 50%;
	position: absolute;
	top: 0px;
	left: 66%;
	background: gray;
}
.box33{
	width: 34%;
	height: 50%;
	position: absolute;
	top: 50%;
	background: burlywood;
	left: 66%;
}
.box3 img{
	width: 100%;
	height: 100%;
	display: block;
	transition: all 0.5s;
}
.box3>div:hover img{
	width: 110%;
	height: 110%;
	margin-top: -5%;
	margin-left: -5%;
}
.box3-con{
	width:170px ;
	text-align: center;
	position: absolute;
	top: 50%;
	left: 50%;
	margin-left: -85px;
	margin-top: -37px;
}
.box3-con h3{
	font-size: 40px;
	color: white;
	font-weight: 500;
	margin-bottom: 10px;
}
.box3-con a{
	font-size: 14px;
	color: white;
}
.box4 .box31{
	left: 34%;
}
.box4 .box32{
	left: 0;
}
.box4 .box33{
	left: 0;
}
.box5{
	position: relative;
	overflow: hidden;
}
.box5 img{
	float: left;
	width: 67%;
}
.box5-r{
	width: 33%;
	text-align: center;
	font-size: 0;
	position: absolute;
	top: 50%;
	margin-top: -120px;
	right: 0px;
}
.box5-r h3{
	font-weight: 500;
	font-size: 28px;
}
.box5-r b{
	font-size: 32px;
	position: relative;
	top: 2px;
}
.line{
	width: 20px;
	margin: 20px auto;
	border-bottom:solid 1px #d39850;
}
.box5-r p{
	color: rgb(119, 119, 119);
    font-size: 13px;
    margin-bottom: 30px;
}
.box5-r input{
	width: 53%;
	height: 48px;
	border: solid 1px #999;
	background: #fcfcfc;
	outline: medium;
	text-indent: 20px;
	margin-right: 3.5%;
}
.box5-r a{
	margin-left: 3.5%;
	width: 17.3%;
	height: 50px;
	background: black;
	color: white;
	font-size: 16px;
	text-align: center;
	display: inline-block;
	vertical-align: top;
	line-height: 50px;
}
.box5-r p:last-child{
	margin-top: 50px;
	margin-bottom: 0px;
}
.icon{
	margin-bottom: 20px;
}
.icon i{
	width: 23px;
	height: 23px;
	display: inline-block;
	background: url(img/ia_100000007.png) -48px -48px;
}
.icon i:nth-child(2){
	background: url(img/ia_100000007.png) -0px -48px;
}
.icon i:nth-child(3){
	background: url(img/ia_100000007.png) -24px -48px;
}
.box6{
	text-align: center;
	padding: 60px 0px 40px;
}
.box6 span{
	margin: 0px 10px;
	font-size: 13px;
}
@media screen and (max-width: 1000px) {
	.hd{
		display: none;
	}
	.hd0{
		display: block;
	}
	.box1{
		overflow: hidden;
		margin-top: 60px;
		height: 275px;
	}
	.box1 h3{
		font-size: 30px;
		width: 90%;
		margin: 70px auto 0px;
	}
	.a1{
		width: 136px;
		height: 45px;
		line-height: 45px;
		margin-top: 30px;
	}
	.box2{
		padding: 40px 0px;
	}
	.box2-con{
		width: 90%;
	}
	.box2-con h3{
		font-size: 30px;
		width: 270px;
		white-space: nowrap;
		overflow: hidden;
	}
	.box2-con p {
	    margin-top: 15px;
	    margin-bottom: 10px;
	}
	.box3{
		height: 177vw;
	}
	.box3 .box31{
		width: 100%;
		height: 118vw;
		left: 0px;
	}
	.box3 .box32{
		width: 50%;
		height: 59vw;
		left: 0px;
		top: 118vw;
	}
	.box3 .box33{
		width: 50%;
		height: 59vw;
		left: 50%;
		top: 118vw;
	}
	.box4 .box31{
		width: 100%;
		height: 118vw;
		left: 0px;
		top: 59vw;
	}
	.box4 .box32{
		width: 50%;
		height: 59vw;
		left: 0px;
		top: 0vw;
	}
	.box4 .box33{
		width: 50%;
		height: 59vw;
		left: 50%;
		top: 0vw;
	}
	.box3-con h3 {
	    font-size: 22px;
	    margin-bottom: 4px;
	}
	.box3-con a {
	    font-size: 13px;
	}
	.box5 img{
		display: block;
		float: none;
		width: 100%;
	}
	.box5-r {
	    width: 100%;
	    text-align: center;
	    font-size: 0;
	    position: static;
	    top: 50%;
	    margin-top: 50px;
	    right: 0px;
	}
	.box5-r input{
		width: 90%;
		margin: 0 ;
	}
	.box5-r a{
		width: 114px;
		display: block;
		margin: 20px auto;
	}
}